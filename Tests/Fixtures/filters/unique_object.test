--TEST--
"unique_by" filter works with list of objects
--TEMPLATE--
{% for i in list|unique_by((i1, i2) => i1 is same as(i2)) %}
 * {{ i.data }}
{% endfor %}
--DATA--
$obj1 = new \StdClass;
$obj1->data = "a";
$obj2 = new \StdClass;
$obj2->data = "a";
$obj3 = new \StdClass;
$obj3->data = "b";
return [ 'list' => [$obj1, $obj2, $obj3, $obj1, $obj3] ];
--EXPECT--
 * a
 * a
 * b
--DATA--
$obj1 = new \StdClass;
$obj1->data = "a";
$obj2 = new \StdClass;
$obj2->data = "b";
$obj3 = new \StdClass;
$obj3->data = "c";
return [ 'list' => new ArrayIterator([$obj1, $obj1, $obj3, $obj2, $obj1]) ];
--EXPECT--
 * a
 * c
 * b
