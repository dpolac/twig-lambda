--TEST--
"filter" filter
--TEMPLATE--
{% for i, n in data|filter(==> _ is even) %}
{{ i }}: {{ n }}
{% else %}
NOTHING
{% endfor %}
--DATA--
return [ 'data' => [ 1, 2, 3, 4, 5, 6 ] ];
--EXPECT--
1: 2
3: 4
5: 6
--DATA--
return [ 'data' => new ArrayIterator([ 1, 2, 3, 4, 5, 6 ]) ];
--EXPECT--
1: 2
3: 4
5: 6
--DATA--
return [ 'data' => [ 1, 1, 3, 21 ] ];
--EXPECT--
NOTHING
--DATA--
return [ 'data' => [ 'a' => 2, 'b' => 2, 'c' => 2 ] ];
--EXPECT--
a: 2
b: 2
c: 2